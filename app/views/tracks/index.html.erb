<h1>Uploaded Tracks</h1>

<% @tracks.each do |track| %>
  <tr>
    <td><%= track.name %></td>
    <td><%= track.description %></td>
    <td><%= track.user_id %></td>
    <td><%= link_to 'Show', track %></td>
    <td><%= link_to 'Edit', edit_track_path(track) %></td>
    <td><%= link_to 'Destroy', track, method: :delete, data: { confirm: 'Is this goodbye?!' } %></td>
  </tr>
<% end %>
</table>

<br>

<%= link_to 'New Track', new_track_path %>