<h1>Members</h1>

<% @users.each do |user| %>
    <h3>Name</h3>
    <p><%= user.name %></p>
    <h3>Email</h3>
    <p><%= user.email %></p>
    <%= link_to 'Show', user %> |
    <%= link_to 'Edit', edit_user_path(user) %> |
    <%= link_to 'Destroy', user, method: :delete, data: { confirm: 'Is this goodbye?!' } %>
<% end %>

<br>
<%= link_to 'New User', new_user_path %>